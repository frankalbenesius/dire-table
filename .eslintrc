{
  "extends": [
    "airbnb",
  ],
  "parser": "babel-eslint",
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": ["**/test.js", "**/*.test.js"]
      }
    ],
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "no-mixed-operators": 0
  }
}
